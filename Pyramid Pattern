//Pyramid pattern
class Pattern{
   public static void main(String args[])
     { for(int i=0;i<10;i++)
	    { for(int j=10-i-1;j>0;j--)
		    {  System.out.print(" ");
			  
			}
			 for(int k=i+1;k>0;k--)
			    { System.out.print("*");
				  System.out.print(" ");
				  
				}
			System.out.println("");
		  
		}
	   
	 }

}
