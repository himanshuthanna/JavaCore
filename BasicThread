class Thread1 extends Thread{
 
   public void run(){
       System.out.println("Scooby");
        
      
     }	  

}
class Thread2 extends Thread{
 
   public void run(){
     
        System.out.println("Shaggy");
      
     }	  

}
 class Exec{
 public static void main(String args[])
      {   
	  
	    Thread1 t1=new Thread1();
	     Thread2 t2=new Thread2();
	     t1.start();
		 t2.start();
	     
	  }
}	  
